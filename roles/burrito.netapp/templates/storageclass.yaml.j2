---
apiVersion: storage.k8s.io/v1
kind: StorageClass
metadata:
  name: netapp
  annotations:
    storageclass.kubernetes.io/is-default-class: "{{ 'true' if storage_backends[0] == 'netapp' else 'false' }}"
provisioner: csi.trident.netapp.io
parameters:
  backendType: "ontap-nas"
  csi.storage.k8s.io/fstype: "ext4"
allowVolumeExpansion: True
...
